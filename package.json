{
  "name": "wa-link",
  "version": "1.0.1",
  "description": "Quickly generate a personalized WhatsApp link with a predefined message and you can share it with your audience on your Social Media!",
  "scripts": {
    "lint": "npm run lint:styles; npm run lint:scripts",
    "lint:styles": "stylelint src",
    "lint:scripts": "eslint src",
    "build:clean": "npm run clean",
    "build:webpack": "webpack --config config/webpack.config.prod.js",
    "build": "cross-env NODE_ENV=production npm-run-all build:*",
    "start": "webpack serve --config config/webpack.config.dev.js",
    "clean": "rimraf build/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gasatrya/wa-link.git"
  },
  "keywords": [],
  "author": "Satrya",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gasatrya/wa-link/issues"
  },
  "homepage": "https://github.com/gasatrya/wa-link#readme",
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.18.6",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.20.0",
    "eslint-webpack-plugin": "^3.2.0",
